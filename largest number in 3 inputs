include <stdio.h>
int main() {
int num1, num2, num3, largest;
scanf("%d %d %d", &num1, &num2, &num3);
if (num1 > num2 && num1 > num3) {
largest = num1;
}
if (num2 > num1 && num2 > num3) {
largest = num2;
}
if (num3 > num1 && num3 > num2) {
largest = num3;
}
printf("The largest of the three numbers is %d\n", largest);
return 0;


